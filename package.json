{
  "name": "directed-graph-editor",
  "version": "1.0.0",
  "description": "Directed graph editor",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --open --mode development",
    "build": "webpack --mode production"
  },
  "keywords": [
    "directed",
    "graph",
    "editor"
  ],
  "author": "Nils Olsson",
  "license": "MIT",
  "dependencies": {
    "cytoscape": "^3.5.5",
    "cytoscape-grid-guide": "^2.2.1",
    "debounce": "^1.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.5",
    "husky": "^2.2.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "always",
    "semi": false,
    "printWidth": 120
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'src/**/*.js'"
    }
  }
}
